ngx_addon_name=ngx_http_pdf_module
if test -n "$ngx_module_link"; then
    ngx_module_type=HTTP_AUX_FILTER
    ngx_module_name=$ngx_addon_name
    ngx_module_incs=
    ngx_module_deps=
    ngx_module_srcs="$ngx_addon_dir/ngx_http_pdf_module.c $ngx_addon_dir/MyWPdfRenderer.cc"
    ngx_module_libs="-lstdc++ -lwt"

    . auto/module
else
HTTP_MODULES="$HTTP_MODULES $ngx_addon_name"
NGX_ADDON_SRCS="$NGX_ADDON_SRCS $ngx_addon_dir/ngx_http_pdf_module.c $ngx_addon_dir/MyWPdfRenderer.cc"
fi
